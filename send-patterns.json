{
  "cvs": {
    "branches": {
      "^master$": {
        "github": {
          "pull_request_merged": {
            "slack": "",
            "message": "*{{.Repo}}:* PR \"{{.PrTitle}} ({{.PrNumber}})\" merged to `{{.BranchRef}}`"
          }
        },

        "circle_ci": {
          "pull_request_merged": {
            "fetch_failed": {},
            "search_failed": {},
            "build_failed": {},
            "wait_failed": {},
            "success": {
              "message": "`{{.Repo}}` PR #{{.PrNumber}} has been built successfully"
            }
          }
        }
      }
    }
  }
}